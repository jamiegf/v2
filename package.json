{
  "name": "mipools-front-end",
  "version": "0.3.1",
  "scripts": {
    "ng": "ng",
    "start-no-ssr": "ng serve --configuration local",
    "start": "ng run mipools-front-end:serve-ssr:local --proxy-config src/proxy.conf.json",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "build:development": "ng build --configuration=development && ng run mipools-front-end:server:development",
    "build:staging": "ng build --configuration=staging && ng run mipools-front-end:server:staging",
    "build:production": "ng build --configuration=production && ng run mipools-front-end:server:production",
    "dev:ssr": "ng run mipools-front-end:serve-ssr",
    "serve:ssr": "node dist/mipools-front-end/server/main.js",
    "build:ssr": "ng build && ng run mipools-front-end:server",
    "prerender": "ng run mipools-front-end:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.1",
    "@angular/cdk": "~17.0.0",
    "@angular/common": "^17.0.1",
    "@angular/compiler": "^17.0.1",
    "@angular/core": "^17.0.1",
    "@angular/forms": "^17.0.1",
    "@angular/material": "~17.0.0",
    "@angular/material-date-fns-adapter": "^17.0.0",
    "@angular/platform-browser": "^17.0.1",
    "@angular/platform-browser-dynamic": "^17.0.1",
    "@angular/platform-server": "^17.0.1",
    "@angular/router": "^17.0.1",
    "@angular/ssr": "^17.0.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "express": "^4.15.2",
    "http-proxy-middleware": "^2.0.6",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.0",
    "@angular-eslint/builder": "17.0.1",
    "@angular-eslint/eslint-plugin": "17.0.1",
    "@angular-eslint/eslint-plugin-template": "17.0.1",
    "@angular-eslint/schematics": "17.0.1",
    "@angular-eslint/template-parser": "17.0.1",
    "@angular/cli": "~17.0.0",
    "@angular/compiler-cli": "^17.0.1",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.1.0",
    "typescript": "~5.2.2"
  }
}
